{
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true },

    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "seeyannewaz.com" }],
      "destination": "https://seeyannewaz.com/:path*",
      "permanent": true
    },

    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "YOUR-PROJECT.vercel.app" }],
      "destination": "https://seeyannewaz.com/:path*",
      "permanent": true
    }
  ]
}
